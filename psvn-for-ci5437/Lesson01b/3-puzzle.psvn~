B X - - - - - - - => X B - - - - - - -  LABEL move00_right
- B X - - - - - - => - X B - - - - - -  LABEL move01_right
- - - B X - - - - => - - - X B - - - -  LABEL move02_right
- - - - B X - - - => - - - - X B - - -  LABEL move03_right
- - - - - - B X - => - - - - - - X B -  LABEL move03_right
- - - - - - - B X => - - - - - - - X B  LABEL move03_right
- - B X => - - X B  LABEL move01_right
B - X - => X - B -  LABEL move00_down
- B - X => - X - B  LABEL move01_down
X B - - => B X - -  LABEL move00_left
- - X B => - - B X  LABEL move01_left
X - B - => B - X -  LABEL move00_up
- X - B => - B - X  LABEL move01_up

